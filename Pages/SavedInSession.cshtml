@page
@model EFDemo.Pages.SavedInSessionModel
@{
    ViewData["Title"] = "Data in session";
}
@if (Model.People != null)
    {
        <p>Dane w zapisane w sesji:</p>
        for(int i=0;i<Model.People.Count();i++)
        {
            <p>@Model.People[i].FirstName @Model.People[i].LastName, @Model.People[i].RokUrodzenia - Rok @Model.People[i].Rok </p>

        }
    }
